<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.safebox">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"></uses-permission>
    <uses-permission android:name="android.permission.CAMERA"></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="gumi"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.LoginRegister.LoginRegisterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.HomePage.HomePageActivity"></activity>
        <activity android:name=".View.Contact.ContactActivity"></activity>
        <activity android:name=".View.Contact.AddContactActivity"></activity>
        <activity android:name=".View.Image.ImageActivity"></activity>
        <activity android:name=".View.Image.ListImageActivity"></activity>
        <activity android:name=".View.Image.ShowImageActivity"></activity>
    </application>

</manifest>